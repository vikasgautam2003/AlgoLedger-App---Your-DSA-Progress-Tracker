{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AlgoLedger",
  "version": "0.2.0",
  "identifier": "com.algoleger.app",

  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },

  "app": {
    "windows": [
      {
        "title": "AlgoLedger",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]

  
  },


  



  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
